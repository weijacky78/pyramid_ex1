<div class="page">
  <h2>Welcome to My App!</h2>
  <p>This is the home page.</p>
  <h3>Pages</h3>
  <ul>
  % for page in pages:
    <li><a href="${request.route_url('page', key=page.key)}">${page.title}</a></li>
  % endfor
  </ul>
  <h3>Photos</h3>
  <ul>
  % for photo in photos:
    <li><img src="${request.static_url('EX1:static/photos/' + photo.filename)}" alt="${photo.description}"></li>
  % endfor
  </ul>
</div>
